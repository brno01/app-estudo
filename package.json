{
  "name": "rest-api",
  "version": "0.0.20",
  "description": "REST API para estudos",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "clean": "rm -rf dist node_modules/.cache",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "update:dependencies": "npm outdated && npm update",
    "release": "node scripts/release.js",
    "release:auto": "node scripts/release-auto.js",
    "release:patch": "npm version patch -m \"chore(release): versão %s (patch)\" && git push && git push --tags",
    "release:minor": "npm version minor -m \"chore(release): versão %s (minor)\" && git push && git push --tags",
    "release:major": "npm version major -m \"chore(release): versão %s (major)\" && git push && git push --tags",
    "release-notes": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "keywords": [
    "rest",
    "node",
    "api"
  ],
  "author": "Bruno Santos de Oliveira",
  "license": "ISC",
  "dependencies": {
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "eslint": "^8.50.0",
    "jest": "^29.0.0",
    "conventional-changelog-cli": "^3.1.31"
  }
}